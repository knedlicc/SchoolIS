# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# Tab indentation (no size specified)
[Makefile]
indent_style = tab

[package.json]
indent_style = space
indent_size = 2

[*.{svelte,js}]
indent_style = space
indent_size = 2

[*.java]
indent_style = space
indent_size = 4
